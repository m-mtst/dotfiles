[color]
	ui = true
[core]
	editor = vim
	excludesfile = ~/.gitignore
  attributesfile = ~/.gitattributes
	autocrlf = input
	whitespace = trailing-space,space-before-tab
[push]
	default = current
[alias]
	apply = apply --whitespace=warn
	rebase = rebase --whitespace=warn
  co = commit
	rh = reset --hard
